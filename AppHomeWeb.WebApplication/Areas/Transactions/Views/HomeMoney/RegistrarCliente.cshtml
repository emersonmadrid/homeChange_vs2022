    <style>
        fieldset.scheduler-border {
            border: solid 2px #DDD !important;
            padding: 0 10px 10px 10px;
            border-bottom: none;
        }

        body {
            /*background-image: url('/Areas/Transactions/Images/FondoRegistro.png');*/
            background-size: cover;
            background-repeat: no-repeat;
            background-position: center center;
            background-attachment: fixed;
        }


        legend.scheduler-border {
            width: auto !important;
            border: none;
            font-size: 16px;
        }

        /*.card-header {
                    padding: .75rem 1.25rem;
                    margin-bottom: 0;
                    background-color: #A9A6A5;
                    border-bottom: 1px solid rgba(0,0,0,.125);
                }

                    .card-header:first-child {
                        border-radius: calc(.25rem - 1px) calc(.25rem - 1px) 0 0;
                        color: white;
                    }*/

        .border-1 {
            border-width: 1px !important;
        }

        .nav > li > a {
            color: #fff;
            text-decoration: none;
            background-color: #9E9C9C;
        }

            .nav > li > a:hover {
                background: #696766;
            }

        .nav-tabs > li.active > a:focus {
            color: white;
            cursor: default;
            background-color: #56d097;
            border: 1px solid #ddd;
            border-bottom-color: transparent;
        }

        .nav-tabs > li.active > a, .nav-tabs > li.active > a:hover {
            color: white;
            cursor: default;
            background-color: #56d097;
            border: 1px solid #ddd;
            border-bottom-color: transparent;
        }

        /*.form-group input[type="checkbox"] {
                    display: none;
                }*/

        .form-group input[type="checkbox"] + .btn-group > label span {
            width: 15px;
        }

            .form-group input[type="checkbox"] + .btn-group > label span:first-child {
                display: none;
            }

            .form-group input[type="checkbox"] + .btn-group > label span:last-child {
                display: inline-block;
            }

        .form-group input[type="checkbox"]:checked + .btn-group > label span:first-child {
            display: inline-block;
        }

        .form-group input[type="checkbox"]:checked + .btn-group > label span:last-child {
            display: none;
        }


        .panel-danger-Claro {
            border: 1px solid #ddd;
        }

            .panel-danger-Claro > .panel-heading {
                color: #FFFCFC;
                background-color: #9E9C9C;
                border-color: #9E9C9C;
            }
    </style>

</head>

<!------ Include the above in your HEAD tag ---------->
<body>
    <div id="ContenedorRegistrarCliente" class="container">
        <div class="row">
            <div class="col-sm-12"><center><h1>Registrate</h1></center></div>
            @*<div class="col-sm-2"><a href="/users" class="pull-right"><img title="profile image" class="img-circle img-responsive" src="http://www.gravatar.com/avatar/28fd20ccec6865e2d5f0e1f4446eb7bf?s=100"></a></div>*@
        </div>
        <div class="row">
            <div class="col-md-2">

            </div><!--/col-3-->
            <div class="col-md-8">
                <div class="panel">


                    <ul id="divDashboardTabs" class="nav nav-tabs" role="tablist" style="cursor: pointer" data-tabs="tabs">
                        <li id="TabDatosCliente" class="active text-center col-md-6 col-xs-6" style="padding: 1px"><a href="#DivDatosCliente" role="tab" data-toggle="tab" data-parent="divDashboardTabs"><span class="glyphicon glyphicon-th-large"></span> Datos</a></li>
                        <li id="TabDatosCuenta" class="text-center col-md-6 col-xs-6" style="padding: 1px"><a href="#DivDatosCuenta" role="tab" data-toggle="tab" data-parent="divDashboardTabs"><span class="glyphicon glyphicon-th-large"></span> Cuenta</a></li>



                    </ul>
                    <div class="tab-content">
                        <div class="bs-example" style="display:none;" id="divalertErrorRegistrar">
                            <div class="alert alert-danger fade in">
                                @*<a href="#" class="close" data-dismiss="alert">&times;</a>*@
                                <strong>Alerta</strong> <span id="lblmensajeAlerta" class=""></span>
                            </div>
                        </div>
                        <div class="bs-example" style="display:none;" id="divalertSuccessRegistrar">
                            <div class="alert alert-success fade in">
                                @*<a href="#" class="close" data-dismiss="alert">&times;</a>*@
                                <strong>Info</strong> <span id="lblmensajeAlertaSuccess" class=""></span>
                            </div>
                        </div>
                        <div class="tab-pane fade active in" id="DivDatosCliente">

                            <form class="form">

                                <div class="panel">
                                    @*<div class="panel panel-heading"> <i class="glyphicon glyphicon-user"></i> Datos del usuario</div>*@
                                    <div class="panel-body">
                                        <div class="form-group">
                                            <div class="col-md-6 col-xs-12">
                                                <label for="cboTipoDocumento"><h5>Tipo Documento</h5></label>
                                                <select id="cboTipoDocumento" class="form-control"></select>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-md-6 col-xs-12">
                                                <label for="txtnum_documento"><h5>Número Documento</h5></label>
                                                <input type="text" class="form-control" name="txtnum_documento" id="txtnum_documento" placeholder="Ingrese Numero de Documento">
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-md-6 col-xs-12">
                                                <label for="txtnombre"><h5>Nombres</h5></label>
                                                <input type="text" class="form-control" name="txtnombre" id="txtnombre" placeholder="Ingrese Nombre">
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-md-6 col-xs-12">
                                                <label for="txtape_paterno"><h5>Apellido Paterno</h5></label>
                                                <input type="text" class="form-control" name="txtape_paterno" id="txtape_paterno" placeholder="Ingrese Apellido Paterno">
                                            </div>
                                        </div>

                                        <div class="form-group">

                                            <div class="col-md-6 col-xs-12">
                                                <label for="txtape_materno"><h5>Apellido Materno</h5></label>
                                                <input type="text" class="form-control" name="txtape_materno" id="txtape_materno" placeholder="Ingrese Apellido Materno">
                                            </div>
                                        </div>

                                    


                                        <div class="form-group">
                                            <div class="col-md-6 col-xs-12">
                                                <label for="cboOcupacion"><h5>Ocupacion</h5></label>
                                                <select id="cboOcupacion" class="form-control"></select>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-md-6 col-xs-12">
                                                <label for="txtnum_celular"><h5>Numero Celular</h5></label>
                                                <input type="text" class="form-control" name="txtnum_celular" id="txtnum_celular" placeholder="Ingrese Numero de Celular">
                                            </div>
                                        </div>


                                        <div class="form-group">
                                            <div class="col-md-6 col-xs-12">
                                                <label for="cboPersonaPublica"><h5>¿Es usted una persona politicamente expuesta?:</h5></label>
                                                <select id="cboPersonaPublica" class="form-control"></select>
                                            </div>
                                        </div>


                                    </div>
                                </div>

                            </form>
                        </div>
                        <div class="tab-pane fade" id="DivDatosCuenta">

                            <form class="form">


                                <div class="panel">
                                    @*  <div class="panel panel-heading"> <i class="glyphicon glyphicon-user"></i> Datos de la Cuenta</div>*@
                                    <div class="panel-body">

                                        <div class="form-group">

                                            <div class="col-md-6 col-xs-12">
                                                <label for="txtemail"><h5>Email:</h5></label>
                                                <input type="email" class="form-control" name="txtemail" id="txtemail" placeholder="you@email.com">
                                            </div>
                                        </div>

                                        <div class="form-group">

                                            <div class="col-md-6 col-xs-12">
                                                <label for="txtclave"><h5>Clave:</h5></label>
                                                <input type="password" class="form-control" name="txtclave" id="txtclave" placeholder="Clave">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-md-6 col-xs-12">
                                                <label for="txtconfirmar"><h5>Confirmar Clave:</h5></label>
                                                <input type="password" class="form-control" name="txtconfirmar" id="txtconfirmar" placeholder="confirmar clave">
                                            </div>
                                        </div>

                                        <div class="row"></div>
                                        <div class="row"></div>
                                    </div>
                                </div>



                            </form>


                        </div>
                        <div id="content" class="panel" style="margin-left:5px; margin-right:5px">

                            <div class="">

                                <div class="">


                                    <center>
                                        <div class="card">
                                            <div class="card-header" style="cursor: pointer">
                                                <button style="padding-left: 35px; padding-right: 35px; margin: 5px; width:150px;" class="btn btn-default" id="btnRegistrar" type="button"><i class="glyphicon glyphicon-floppy-saved"></i>  Guardar</button>

                                            </div>
                                        </div>
                                    </center>
                                </div>
                            </div>
                        </div>
                    </div><!--/tab-pane-->

                </div>
            </div><!--/tab-pane-->
        </div>
        <div class="col-md-2">
        </div><!--/tab-pane-->
    </div>

</body>
</html>
@Scripts.Render("~/bundles/bootstrap")
@Scripts.Render("~/bundles/jquery-addon-home")
@Scripts.Render("~/bundles/Home-Struct")
@Scripts.Render("~/bundles/HomeMoneyLib ")
@Scripts.Render("~/bundles/RegistrarCliente")
